{
    "name": "spartan/cache",
    "description": "Spartan Cache library",
    "type": "library",
    "keywords": [
        "library",
        "spartan",
        "cache"
    ],
    "homepage": "https://spartanphp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Iulian N.",
            "email": "iulian@spartanphp.com",
            "homepage": "https://spartanphp.com",
            "role": "developer"
        }
    ],
    "support": {
        "email": "support@spartanphp.com",
        "docs": "https://docs.spartanphp.com"
    },
    "require": {
        "php": ">=8.0",
        "laminas/laminas-cache": "^3.9"
    },
    "suggest": {
        "laminas/laminas-cache-storage-adapter-filesystem": "^2.0",
        "laminas/laminas-cache-storage-adapter-apcu": "^2.0",
        "laminas/laminas-cache-storage-adapter-memcached": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Spartan\\Cache\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "spartan/service": "^0.1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Spartan\\Cache\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
    },
    "extra": {
        "spartan": {
            "install": {
                "env": {
                    "CACHE_ADAPTER": "memory",
                    "CACHE_TTL": "60"
                },
                "copy": {
                    "config/cache.php": "config/cache.php"
                },
                "commands": [
                    "__DIR__ . '/../vendor/spartan/cache/src/Command'"
                ],
                "services": [
                    "Spartan\\Cache\\ServiceProvider::class"
                ]
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
